generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Datas {
  id       Int     @id @default(autoincrement())
  day      String
  month    String
  location String
  city     String
  linkbuy  String?

  @@map("datas")
}

model VideoLink {
  id   Int    @id @default(autoincrement())
  link String
}

model Admin {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("admins")
}
